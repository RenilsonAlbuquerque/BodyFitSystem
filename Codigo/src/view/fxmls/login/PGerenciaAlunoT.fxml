<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<HBox prefHeight="589.0" prefWidth="797.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox fx:id="painelEsquerda" prefHeight="588.0" prefWidth="400.0" spacing="20.0">
         <children>
            <ListView fx:id="listaAlunos" onMouseClicked="#acaoMudancaItens" prefHeight="431.0" prefWidth="347.0" />
            <TextField fx:id="txtBuscar" alignment="CENTER" prefHeight="36.0" prefWidth="350.0" />
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
      <VBox fx:id="painelDireita" prefHeight="576.0" prefWidth="400.0">
         <children>
            <HBox fx:id="painelBaixo" alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
               <children>
                  <Button fx:id="btnAdcionar" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="64.0" prefWidth="105.0" text="Adcionar Treino" />
                  <Button fx:id="btnEditar" mnemonicParsing="false" prefHeight="64.0" prefWidth="92.0" text="Editar Treino" />
                  <Button fx:id="btnExcluir" mnemonicParsing="false" prefHeight="64.0" prefWidth="92.0" text="Excluir Treino" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</HBox>
