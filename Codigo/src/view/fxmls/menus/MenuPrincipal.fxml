<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.effect.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.shape.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<BorderPane prefHeight="526.0" prefWidth="863.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <GridPane fx:id="painelTitulos" prefHeight="86.0" prefWidth="764.0" BorderPane.alignment="CENTER">
        <columnConstraints>
            <ColumnConstraints halignment="LEFT" maxWidth="605.0" minWidth="10.0" prefWidth="139.0" />
          <ColumnConstraints halignment="LEFT" maxWidth="628.0" minWidth="10.0" prefWidth="560.0" />
            <ColumnConstraints halignment="RIGHT" hgrow="ALWAYS" maxWidth="620.0" minWidth="10.0" prefWidth="176.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints fillHeight="false" minHeight="130.0" prefHeight="30.0" valignment="CENTER" vgrow="ALWAYS" />
        </rowConstraints>
         <children>
            <Circle fx:id="fotoPerfil" fill="#f9fbfc" onMouseClicked="#acaoBotaoPerfil" radius="60.0" stroke="BLACK" strokeType="INSIDE" />
            <Label fx:id="labelNomeUsuario" prefHeight="59.0" prefWidth="395.0" textFill="WHITE" GridPane.columnIndex="1">
               <font>
                  <Font name="Arial Black" size="40.0" />
               </font>
            </Label>
            <HBox alignment="CENTER_RIGHT" prefHeight="90.0" prefWidth="141.0" spacing="10.0" GridPane.columnIndex="2">
               <children>
                  <Button fx:id="botaoVoltar" mnemonicParsing="false" onAction="#acaoBotaoVoltar" prefHeight="48.0" prefWidth="36.0">
                     <graphic>
                        <ImageView fitHeight="33.0" fitWidth="31.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@/imagens/voltar.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button fx:id="botaoTrocarPerfil" mnemonicParsing="false" onAction="#acaoBotaoTrocarPerfil" prefHeight="48.0" prefWidth="36.0">
                     <graphic>
                        <ImageView fitHeight="33.0" fitWidth="31.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@/imagens/trocarUsuario.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button fx:id="botaoSair" mnemonicParsing="false" onAction="#acaoBotaoSair" prefHeight="48.0" prefWidth="36.0">
                     <graphic>
                        <ImageView fitHeight="33.0" fitWidth="31.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@/imagens/logout.png" />
                           </image>
                        </ImageView>
                     </graphic></Button>
               </children>
            </HBox>
         </children>
      </GridPane>
   </top>
</BorderPane>
